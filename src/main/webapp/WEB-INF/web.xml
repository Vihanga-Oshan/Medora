<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Abc</servlet-name>
        <servlet-class>com.example.base.controller.AddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Abc</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterPatientServlet</servlet-name>
        <servlet-class>com.example.base.controller.registerPatientServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegisterPatientServlet</servlet-name>
        <url-pattern>/RegisterPatientServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterGuardianServlet</servlet-name>
        <servlet-class>com.example.base.controller.RegisterGuardianServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegisterGuardianServlet</servlet-name>
        <url-pattern>/RegisterGuardianServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>sqr</servlet-name>
        <servlet-class>com.example.base.controller.sqServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>sqr</servlet-name>
        <url-pattern>/sq</url-pattern>
    </servlet-mapping>

</web-app>
